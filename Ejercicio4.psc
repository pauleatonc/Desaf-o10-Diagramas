	Escribir "comparemos tres numeros. Escribe el primero"
	Leer a
	Escribir "escribe el segundo numero"
	leer b
	Escribir "escribe el ultimo numero"
	leer c
	
	si a > c Entonces 
		si a > b Entonces
			Escribir a " es el numero mayor"
			Sino
				Escribir b " es el numero mayor"
